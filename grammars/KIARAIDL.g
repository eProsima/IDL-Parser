    ;

WS
    :   (' ' | '\r' | '\t' | '\u000C' | '\n') -> channel(HIDDEN)
    ;
	
PREPROC_DIRECTIVE
	:
	'#'
	(~'\n')* '\n'
	{ 
		ctx.processPreprocessorLine(new String(getText()), getLine());
		skip();
		//newline();
	}
	;
	
COMMENT
