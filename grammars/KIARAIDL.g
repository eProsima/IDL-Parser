event
@init{
    System.out.println("WARNING (File " + ctx.getFilename() + ", Line " + (_input.LT(0) != null ? _input.LT(0).getLine() - ctx.getCurrentIncludeLine() : "1") + "): Event declarations are not supported. Ignoring...");
