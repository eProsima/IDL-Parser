type_12(package) ::= <<long>>

type_8(package) ::= <<char>>

type_14(package) ::= <<wchar_t>>

type_7(package) ::= <<boolean>>

type_9(package) ::= <<byte>>

type_d(package) ::= <<String>>

type_15(package) ::= <<std::wstring>>

type_f_first(prev) ::= <<std::array<$prev$>>
